{
    "Version":"2012-10-17",
    "Statement":[
      {
        "Sid": "PolicyForAllowUploadWithACL",
        "Effect": "Allow",
        "Principal": {
            "AWS": "arn:aws:iam::414057778078:user/github"
        },
        "Action": [
            "s3:DeleteObject",
            "s3:GetBucketLocation",
            "s3:GetObject",
            "s3:ListBucket",
            "s3:PutObject"
        ],
        "Resource": "arn:aws:s3:::${bucket}/*",
        "Condition": {
          "StringEquals": {"s3:x-amz-acl":"bucket-owner-full-control"}
        }
      }
    ]
 }